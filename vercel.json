{
  "version": 2,
  "builds": [
    { "src": "**/*.php", "use": "@vercel/php" },
    { "src": "css/**", "use": "@vercel/static" },
    { "src": "js/**", "use": "@vercel/static" },
    { "src": "images/**", "use": "@vercel/static" },
    { "src": "img/**", "use": "@vercel/static" },
    { "src": "style/**", "use": "@vercel/static" },
    { "src": "sbadmin/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(css|js|images|img|style|sbadmin|load|pictures)/(.*)", "dest": "/$1/$2" },
    { "src": "/(.*\\.php)$", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.php" }
  ]
}
